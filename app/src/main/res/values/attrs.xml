<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ShadeTabView">
        <attr name="normalIcon" format="reference" />
        <attr name="selectIcon" format="reference" />
        <attr name="tab" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="normalTextColor" format="color" />
        <attr name="selectTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="AppTitle">
        <attr name="titletype" format="integer" />
        <attr name="leftIcon" format="reference" />
        <attr name="title" format="string" />
        <attr name="menuIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="SlideMenu">
        <attr name="sm_type" format="integer" />
        <attr name="sm_dragoffset" format="dimension" />
        <attr name="sm_menuoffset" format="dimension" />
    </declare-styleable>
</resources>